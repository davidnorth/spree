<%= render :partial => 'admin/shared/order_tabs', :locals => {:current => "Payments"} %>

<h2><%= t("new_credit_card_payment")%></h2>

<%= error_messages_for :creditcard_payment %>
<% form_for @creditcard_payment, :url => collection_url do |f| %>

  <%= render "form", :f => f %>

  <p class="form-buttons">
    <%= button t('continue') %>
  </p>

<% end %>

<% content_for :head do %>
<%= javascript_include_tag 'admin/payments/new' %>
<% end %>